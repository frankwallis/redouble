    "koa-webpack-hot-middleware": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "reify": "^0.12.3",
    "rimraf": "^2.4.4",
    "s3": "^4.4.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "webpack": "^3.6.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "scripts": {
    "clean": "bsb -clean-world && rimraf ./dist",
    "build": "webpack --config config/webpack.config.js",
    "build:dev": "webpack --config config/webpack.config.js --env.development",
    "build:watch": "webpack --config config/webpack.config.js --env.development --watch",
    "compile": "bsb -make-world",
    "compile:watch": "bsb -make-world -w",
    "deploy": "npm run build && node ./config/aws-deploy.js && npm run clean",
    "serve": "node --require reify ./src/server/main.js",
    "start": "npm run compile && npm run serve",
    "test": "jest --config ./config/jest.config.js",
    "test:watch": "jest --config ./config/jest.config.js --watchAll",
    "test:coverage": "jest --config ./config/jest.config.js --collectCoverage"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/frankwallis/redouble.git"
  },
  "engines": {
    "node": ">= 6.9.1"
